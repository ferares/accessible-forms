{
    "env": {
      "browser": true,
      "jquery": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "no-var": 0,
      "prefer-const": 1,
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "array-bracket-spacing": [
        "error",
        "always"
      ],
      "space-in-parens": [
        "error",
        "never"
      ],
      "no-console": [
        "off"
      ]
    }
}
